<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Lifecycle xmlns="http://schemas.dgit.biz/telflow/cim/v3">
  <ID>TR-ORD-LCX002</ID>
  <Name>Training Order Lifecycle with CPQ</Name>
  <ValidFor>
    <StartDateTime>2020-04-09T00:00:00.000Z</StartDateTime>
  </ValidFor>
  <Version>1.0</Version>
  <Status>Active</Status>
  <LifecycleStates>
    <LifecycleState>
      <ID>Draft</ID>
      <Name>Draft</Name>
      <Type>Draft</Type>
    </LifecycleState>
    <LifecycleState>
      <ID>Review</ID>
      <Name>Review</Name>
      <Type>Pipeline</Type>
    </LifecycleState>
    <LifecycleState>
      <ID>Acknowledged</ID>
      <Name>Acknowledged</Name>
      <Type>Start</Type>
    </LifecycleState>
    <LifecycleState>
      <ID>In Progress</ID>
      <Name>In Progress</Name>
      <Type>Intermediate</Type>
    </LifecycleState>
    <LifecycleState>
      <ID>Complete</ID>
      <Name>Complete</Name>
      <Type>EndSuccess</Type>
    </LifecycleState>
    <LifecycleState>
      <ID>Cancel</ID>
      <Name>Cancel</Name>
      <Type>EndFailure</Type>
    </LifecycleState>
  </LifecycleStates>
  <Transitions>
    <Transition>
      <Name>Save Draft</Name>
      <Output>
        <ID>Draft</ID>
      </Output>
      <PartyRoleTypes>
        <PartyRoleType>Provider</PartyRoleType>
        <PartyRoleType>System</PartyRoleType>
      </PartyRoleTypes>
    </Transition>
    <Transition>
      <Name>Review</Name>
      <Input>
        <ID>Draft</ID>
      </Input>
      <Output>
        <ID>Review</ID>
      </Output>
      <PartyRoleTypes>
        <PartyRoleType>Provider</PartyRoleType>
        <PartyRoleType>System</PartyRoleType>
      </PartyRoleTypes>
    </Transition>
    <Transition>
      <Name>Proceed</Name>
      <Input>
        <ID>Review</ID>
      </Input>
      <Output>
        <ID>Acknowledged</ID>
      </Output>
      <PartyRoleTypes>
        <PartyRoleType>Provider</PartyRoleType>
        <PartyRoleType>System</PartyRoleType>
      </PartyRoleTypes>
    </Transition>
    <Transition>
      <Name>Submit</Name>
      <Output>
        <ID>Acknowledged</ID>
      </Output>
      <PartyRoleTypes>
        <PartyRoleType>Provider</PartyRoleType>
        <PartyRoleType>System</PartyRoleType>
      </PartyRoleTypes>
    </Transition>
    <Transition>
      <Name>Proceed Order</Name>
      <Input>
        <ID>Acknowledged</ID>
      </Input>
      <Output>
        <ID>In Progress</ID>
      </Output>
      <PartyRoleTypes>
        <PartyRoleType>System</PartyRoleType>
      </PartyRoleTypes>
    </Transition>
    <Transition>
      <Name>Complete</Name>
      <Input>
        <ID>In Progress</ID>
      </Input>
      <Output>
        <ID>Complete</ID>
      </Output>
      <PartyRoleTypes>
        <PartyRoleType>System</PartyRoleType>
      </PartyRoleTypes>
    </Transition>
    <Transition>
      <Name>Cancel</Name>
      <Input>
        <ID>In Progress</ID>
      </Input>
      <Output>
        <ID>Cancel</ID>
      </Output>
      <PartyRoleTypes>
        <PartyRoleType>System</PartyRoleType>
      </PartyRoleTypes>
    </Transition>
  </Transitions>
  <Permissions>
    <Permission>
      <State>
        <ID>Acknowledged</ID>
      </State>
      <AccessLevel>ReadUpdate</AccessLevel>
      <PartyRoleTypes>
        <PartyRoleType>Provider</PartyRoleType>
        <PartyRoleType>System</PartyRoleType>
      </PartyRoleTypes>
    </Permission>
    <Permission>
      <State>
        <ID>In Progress</ID>
      </State>
      <AccessLevel>ReadUpdate</AccessLevel>
      <PartyRoleTypes>
        <PartyRoleType>Provider</PartyRoleType>
        <PartyRoleType>System</PartyRoleType>
      </PartyRoleTypes>
    </Permission>
    <Permission>
      <State>
        <ID>Complete</ID>
      </State>
      <AccessLevel>ReadUpdate</AccessLevel>
      <PartyRoleTypes>
        <PartyRoleType>Provider</PartyRoleType>
        <PartyRoleType>System</PartyRoleType>
      </PartyRoleTypes>
    </Permission>
    <Permission>
      <State>
        <ID>Cancel</ID>
      </State>
      <AccessLevel>ReadUpdate</AccessLevel>
      <PartyRoleTypes>
        <PartyRoleType>Provider</PartyRoleType>
        <PartyRoleType>System</PartyRoleType>
      </PartyRoleTypes>
    </Permission>
  </Permissions>
  <AvailableFor>
    <BusinessInteractionType>CustomerOrder</BusinessInteractionType>
  </AvailableFor>
</Lifecycle>
